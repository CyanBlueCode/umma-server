{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": "warn",
      "no-console": "off",
      "func-names": "off",
      "object-shorthand": "off",
      "class-methods-use-this": "off",
      "jsx-a11y/anchor-is-valid": "off",
      "jsx-a11y/anchor-has-content": "off",
      "jsx-a11y/control-has-associated-label": "off"
    },
    "env": {
      "browser": true,
      "node": true
    },
    "parser": "babel-eslint"
  }
  