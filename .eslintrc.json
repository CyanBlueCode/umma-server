{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": "warn",
      // "func-names": "off",
      "no-console": ["error", { "allow": ["warn", "error", "info"] }]
    },
    "env": {
      "browser": true,
      "node": true
    },
    "parser": "babel-eslint"
  }
  